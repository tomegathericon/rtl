# Configure imap username and password 
 set imap_user="kapoor.a@impressico.com"
 set imap_pass="pass123#$"
#
# # Configure Default Folder and where to put Sent Mail 
 set folder="imaps://imap.gmail.com:993/"
 set spoolfile=$folder
 set record="=Sent"
#
# # Configure SMTP Settings 
 set smtp_url = "smtps://$imap_user:$imap_pass@smtp.gmail.com:465/"
 set realname="Kapoor Abhishek"
 set from="kapoor.a@impressico.com"
 set use_from = yes
